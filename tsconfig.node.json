{
  "compilerOptions": {
    "jsx": "react",
    "composite": true,
    "skipLibCheck": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "resolveJsonModule": true,
    "strict": true, // Adicionando a opção strict
    "forceConsistentCasingInFileNames": true // Adicionando a opção forceConsistentCasingInFileNames
  },
  "include": [
    "./config/"
  ]
}
